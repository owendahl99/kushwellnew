{% extends "base.html" %}
{% block content %}
<div class="container">
  <div class="error-wrap">
    <h1 class="error-title">Something went wrong</h1>
    <p class="error-sub">{{ message|default("An unexpected error occurred.") }}</p>
    <div class="error-actions">
      <a class="kw-btn kw-btn--primary" href="/">Go home</a>
      <a class="kw-btn kw-btn--outline" href="javascript:location.reload()">Try again</a>
    </div>
  </div>
</div>
{% endblock %}

