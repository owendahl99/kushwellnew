{% extends "base.html" %}
{% block content %}
<div class="container">
  <div class="error-wrap">
    <h1 class="error-title">Page not found</h1>
    <p class="error-sub">{{ message|default("The page you’re looking for doesn’t exist.") }}</p>
    <div class="error-actions">
      <a class="kw-btn kw-btn--primary" href="/">Go home</a>
      <a class="kw-btn kw-btn--outline" href="javascript:history.back()">Back</a>
    </div>
  </div>
</div>
{% endblock %}

